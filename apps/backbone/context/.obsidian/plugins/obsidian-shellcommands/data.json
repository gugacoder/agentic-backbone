{
  "settings_version": "0.22.0",
  "debug": false,
  "obsidian_command_palette_prefix": "Shell: ",
  "shell_commands": {
    "backbone-dev": {
      "id": "backbone-dev",
      "shell_command": "cd {{vault_path}}/../../.. && npm run dev:backbone",
      "alias": "Start Backbone (dev)",
      "confirm_execution": true,
      "output_channels": {
        "stdout": "notification",
        "stderr": "notification"
      },
      "shells": {
        "default": "cd {{vault_path}}/../../.. && npm run dev:backbone"
      }
    },
    "backbone-build": {
      "id": "backbone-build",
      "shell_command": "cd {{vault_path}}/../../.. && npm run build --workspace=apps/backbone",
      "alias": "Build Backbone",
      "confirm_execution": true,
      "output_channels": {
        "stdout": "notification",
        "stderr": "notification"
      },
      "shells": {
        "default": "cd {{vault_path}}/../../.. && npm run build --workspace=apps/backbone"
      }
    }
  }
}
